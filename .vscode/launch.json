{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Cortex Debug",
      "cwd": "${workspaceRoot}",
      "executable": "${workspaceRoot}/build/${workspaceFolderBasename}.elf",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "openocd",
      "device": "STM32F411VE",
      "configFiles": [
        "interface/stlink.cfg",
        "target/stm32f4x.cfg"
      ],
      "preLaunchTask": "flash",
      "showDevDebugOutput": "raw",
      "svdFile": "${workspaceRoot}/debug-files/STM32F411.svd"
    }
  ]
}